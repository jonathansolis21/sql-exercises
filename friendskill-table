CREATE TABLE [dbo].[FriendSkill](
	[FriendId] [int] NOT NULL,
	[SkillId] [int] NOT NULL,
 CONSTRAINT [PK_FriendSkill] PRIMARY KEY CLUSTERED 
(
	[FriendId] ASC,
	[SkillId] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]
GO

ALTER TABLE [dbo].[FriendSkill]  WITH CHECK ADD  CONSTRAINT [FK_FriendSkill_Skills] FOREIGN KEY([SkillId])
REFERENCES [dbo].[Skills] ([SkillTableId])
GO

ALTER TABLE [dbo].[FriendSkill] CHECK CONSTRAINT [FK_FriendSkill_Skills]
GO
