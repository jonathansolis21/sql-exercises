BEGIN
	
	DECLARE @ImageId int

	INSERT INTO dbo.Images 
    ([TypeId]
    , [Url])
	VALUES (@TypeId
    ,@Url)											
																																																			
	DECLARE @LatestImageId as int = SCOPE_IDENTITY()	

	INSERT INTO dbo.Friends (Title
    ,Bio
    ,Summary
    ,Headline 
    ,Slug
    ,StatusId 
    ,PrimaryImageId)
  VALUES	(@Title
